#include <stdio.h>
int main(){
	int n, key ,chk=0;

	scanf("%d", &n);
	int arr[n];

	for (int i=0; i<n; i++){
		scanf("%d", &arr[i]);

	}
	scanf("%d", &key);

	for (int i  = 0; i<n; i++){
		if (arr [i] == key){
			printf("found at position %d\n", i);
			chk=1;
			break;
		}
	}
		if (!chk){
			printf("%d not found\n", key);
	}
	return 0;
}
